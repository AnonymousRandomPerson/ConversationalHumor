    "version": "0.2.0",
    "configurations": [
        {
            "name": "qlearn",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/humor/qlearn.py",
            "cwd": "${workspaceFolder}/humor",
            "debugOptions": [
                "RedirectOutput"
            ],
            "args": [
                "-m",
                "evaluated_rl",
                "--save_dir",
                "chatbot_rnn/models/reddit"
            ]
        },
        {
            "name": "DeepQA",
            "type": "python",
            "request": "launch",
            "program": "${workspaceFolder}/humor/DeepQA/main.py",
            "cwd": "${workspaceFolder}/humor",
            "module": "DeepQA.main",
            "debugOptions": [
                "RedirectOutput"
            ],
            "args": [
                "--modelTag",
                "cornell",
                "--test",
                "interactive",
                "--rootDir",
                "DeepQA"
            ]
        },
    ]
}